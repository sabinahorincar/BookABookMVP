<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.bookabook.ui.home.HomeFragment"
        android:label="BookABook"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.bookabook.ui.gallery.GalleryFragment"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_mycart"
        android:name="com.example.bookabook.ui.myCart.MyCartFragment"
        tools:layout="@layout/fragment_mycart" />


    <fragment
        android:id="@+id/nav_login"
        android:name="com.example.bookabook.ui.login.LogInFragment"
        tools:layout="@layout/fragment_login" />
    <fragment
        android:id="@+id/nav_register"
        android:name="com.example.bookabook.ui.register.RegisterFragment"
        tools:layout="@layout/fragment_register" />
</navigation>